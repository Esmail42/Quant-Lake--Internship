    "# prime number checker"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "f31d4a91-e18b-4183-8214-e67340d919fb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Is 17 a prime number? True\n",
      "Is 28 a prime number? False\n"
     ]
    }
   ],
   "source": [
    "def is_prime(n):\n",
    "    if n < 2:\n",
    "        return False\n",
    "    for i in range(2, int(n ** 0.5) + 1):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "print(\"Is 17 a prime number?\", is_prime(17))\n",
    "print(\"Is 28 a prime number?\", is_prime(28))"
   ]
  },
  {
